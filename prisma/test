
model Event {
  event_id          Int       @id @default(autoincrement())
  created_by        Int       
  event_title       String
  event_description String?
  start_date        DateTime
  end_date          DateTime
  created_at        DateTime @default(now())

  @@map("tbl_events")
}

model User {
    user_id    Int      @id @default(autoincrement())
    username   String   @unique
    password   String
    email      String   @unique
    role       Role
    created_at DateTime @default(now())
  
    @@map("tbl_users")
  }